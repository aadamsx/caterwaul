sdocp('test/tostring-identity.js.sdoc', 'Serialization via serialize()\n\ntest(function () {\n  var n = function (s)    {return caterwaul.lex(s).join(\' \').replace(/;\\s*\\}/g, \'}\').replace(/^\\s+|\\s+$/g, \'\')},\n      s = function (s, i) {return eq(caterwaul.parse(s).serialize(), i)},\n      i = function (f)    {return eq(n(caterwaul.decompile(f).serialize()),\n                                     n(f.toString()))};\n\n  Simple tests:\n\n  i(function(){return 5});\n  i(function(x){return function(y) {return x + y}});\n  i(function () {var x = 5; return x});\n});\n');