sdocp('test/lex-end-failure.js.sdoc', 'Mysterious \'end()\' bug -- caused by a lexer bug that interpreted \'.e\' as a number in scientific notation\n\ntest(function () {\n  var l = caterwaul.lex;\n\n  aeq(l(\'foo.bar()\'), [\'foo\', \'.\', \'bar\', \'(\', \')\']);\n  aeq(l(\'foo.e()\'), [\'foo\', \'.\', \'e\', \'(\', \')\']);\n  aeq(l(\'foo.end()\'), [\'foo\', \'.\', \'end\', \'(\', \')\']);\n});\n');