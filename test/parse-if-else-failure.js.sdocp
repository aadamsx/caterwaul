sdocp('sdoc::js::test/parse-if-else-failure', 'An obscure failure case involving if/else nested inside a blockless for.\n\ntest(\'parse-if-else-failure\', function () {\n  var c = caterwaul.clone(\'std\');\n  var f = c(function (x) {\n    if (x === 5) for (var i = 0; i < x; ++i) {if (i === 3) return i;\n                                              else         x = x}\n    else if (x === 3) return x + 10;\n    else return x + 1;\n  });\n\n  eq(f(0), 1);\n  eq(f(3), 13);\n  eq(f(5), 3);\n});');