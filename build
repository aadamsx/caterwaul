#!/bin/bash
sdoc . test/
rm -f .test-log
for file in test/*.js; do
  echo $file
  ${js:-d8} divergence.improved.js unit.js $file
  if [[ $? -eq 0 ]]; then echo pass $file >> .test-log
  else                    echo fail $file >> .test-log
                          echo -e "\033[1;31mfail\033[0;0m: $file"; fi
done
