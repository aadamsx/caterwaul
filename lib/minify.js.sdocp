sdocp('lib/minify.js.sdoc', 'Minifies a JavaScript file by using the Caterwaul parse/deparse mechanism.\nThis won\'t do identifier packing, but it will provide decent minification by removing comments and whitespace.\n\neval(require(\'fs\').readFileSync(\'caterwaul.js\', \'utf8\'));\n\nvar stdin = process.openStdin();\nvar code  = \'\';\nstdin.setEncoding(\'utf8\');\nstdin.on(\'data\', function (chunk) {code += chunk});\nstdin.on(\'end\',  function      () {\n  var tree = caterwaul.parse(code);\n  process.stdout.write(tree.serialize());\n});\n');