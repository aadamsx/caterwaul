sdocp('sdoc::js::modules/caterwaul.seq.test/finite.filter', 'Finite sequence filter tests.\n\ntest(\'caterwaul.seq.finite.filter\', function () {\n  var c = caterwaul.clone(\'std seq\');\n\n  c(function (eq) {\n    var xs = new caterwaul.seq.finite([1, 2, 3, 4, 5]);\n    var evens = xs.filter(fn[x][! (x & 1)]);\n\n    eq(evens.size(), 2);\n    eq(evens[0], 2);\n    eq(evens[1], 4);\n\n    var odds = xs.filter(fn[x][x & 1]);\n\n    eq(odds.size(), 3);\n    eq(odds[0], 1);\n    eq(odds[1], 3);\n    eq(odds[2], 5);\n\n    var first_three = xs.filter(fn[x, i][i < 3]);\n\n    eq(first_three.size(), 3);\n    eq(first_three[0], 1);\n    eq(first_three[1], 2);\n    eq(first_three[2], 3);\n  })(eq);\n});');