sdocp('sdoc::js::modules/caterwaul.parser.test/lookahead', 'Tests for lookahead assertions.\n\ntest(\'caterwaul.parser.lookahead\', function () {\n  var c = caterwaul.clone(\'std parser\');\n\n  c(function (eq) {\n    var p = caterwaul.parser.parsers;\n    \n    var aa = p.seq(p.c(\'a\'), p.match(p.c(\'a\')), p.alt(p.c(\'a\'), p.c(\'b\')));\n    eq(aa(\'aa\').result.join(\',\'), \'a,a,a\');\n    eq(aa(\'ab\'), false);\n    eq(aa(\'a\'), false);\n\n    var ab = p.seq(p.c(\'a\'), p.reject(p.c(\'a\')), p.alt(p.c(\'a\'), p.c(\'b\')));\n    eq(ab(\'ab\').result.length, 3);\n    eq(ab(\'ab\').result[0], \'a\');\n    eq(ab(\'ab\').result[1], null);\n    eq(ab(\'ab\').result[2], \'b\');\n    eq(ab(\'aa\'), false);\n    eq(ab(\'a\'), false);\n  })(eq);\n});');