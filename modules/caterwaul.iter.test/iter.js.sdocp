sdocp('modules/caterwaul.iter.test/iter.js.sdoc', 'Tests for iter.n and iter.keys\n\ntest(function () {\n  var f = caterwaul.clone(\'std\', \'iter\')(function (eq) {\n    var iterations = 0;\n    iter.n[i, 10][++iterations];\n    eq(iterations, 10);\n    iter.n[i, 8][++iterations];\n    eq(iterations, 18);\n    iter.n[i, 7][++iterations];\n    eq(iterations, 25);\n    iter.n[i, 1][++iterations];\n    eq(iterations, 26);\n    iter.n[i, 24][++iterations];\n    eq(iterations, 50);\n\n    var object = {foo: \'bar\', bif: \'baz\'};\n    var s = \'\';\n    iter.keys[k, object][s += k];\n    eq(s, \'foobif\');\n    iter.keys[k, object][s += object[k]];\n    eq(s, \'foobifbarbaz\');\n\n    var i = 0;\n    iter.until[i >= 100][++i];\n    eq(i, 100);\n\n    return iterations;\n  });\n\n  var iterations = f(eq);\n  eq(iterations, 50);\n});\n');