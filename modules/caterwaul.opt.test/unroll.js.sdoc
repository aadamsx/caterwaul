Tests for opt.unroll

test(function () {
  var f = caterwaul.clone('std', 'opt.unroll')(function (eq) {
    var iterations = 0;
    opt.unroll[i, 10][++iterations];
    eq(iterations, 10);
    opt.unroll[i, 8][++iterations];
    eq(iterations, 18);
    opt.unroll[i, 7][++iterations];
    eq(iterations, 25);
    opt.unroll[i, 1][++iterations];
    eq(iterations, 26);
    opt.unroll[i, 24][++iterations];
    eq(iterations, 50);
    return iterations;
  });

  var iterations = f(eq);
  eq(iterations, 50);
});
